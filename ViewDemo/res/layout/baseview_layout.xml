<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin" >

    <RelativeLayout
        android:id="@+id/myLinearLayout"
        android:layout_width="fill_parent"
        android:layout_height="40dip"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/firsstTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="TextView"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/more_setting_about_go"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/actionbar_go_normal" />
    </RelativeLayout>

    <ImageView
        android:id="@+id/img1"
        android:layout_width="match_parent"
        android:layout_height="2dip"
        android:layout_below="@+id/myLinearLayout"
        android:background="@drawable/line" />

    <RelativeLayout
        android:id="@+id/myFrameLayout"
        android:layout_width="fill_parent"
        android:layout_height="40dip"
        android:layout_below="@+id/img1"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/firsstTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="EditText"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/more_setting_about_go"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/actionbar_go_normal" />
    </RelativeLayout>

    <ImageView
        android:id="@+id/img2"
        android:layout_width="match_parent"
        android:layout_height="2dip"
        android:layout_below="@+id/myFrameLayout"
        android:background="@drawable/line" />

    <RelativeLayout
        android:id="@+id/myTableLayout"
        android:layout_width="fill_parent"
        android:layout_height="40dip"
        android:layout_below="@+id/img2"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/firsstTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="ImageButton"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/more_setting_about_go"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/actionbar_go_normal" />
    </RelativeLayout>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="2dip"
        android:background="@drawable/line" />

    <RelativeLayout
        android:id="@+id/myRelativeLayout"
        android:layout_width="fill_parent"
        android:layout_height="40dip"
        android:layout_below="@+id/myTableLayout"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/firsstTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="Checkbox"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/more_setting_about_go"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/actionbar_go_normal" />
    </RelativeLayout>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="2dip"
        android:background="@drawable/line" />

    <RelativeLayout
        android:id="@+id/myAbsoluteLayout"
        android:layout_width="fill_parent"
        android:layout_height="40dip"
        android:layout_below="@+id/myRelativeLayout"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/firsstTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="RadioButton"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/more_setting_about_go"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/actionbar_go_normal" />
    </RelativeLayout>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="2dip"
        android:background="@drawable/line" />

    <RelativeLayout
        android:id="@+id/myView"
        android:layout_width="fill_parent"
        android:layout_height="40dip"
        android:layout_below="@+id/myAbsoluteLayout"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/firsstTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="ImageView"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/more_setting_about_go"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/actionbar_go_normal" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/myexample"
        android:layout_width="fill_parent"
        android:layout_height="40dip"
        android:layout_below="@+id/myView"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/firsstTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="AnalogClock和DigitalClock"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/more_setting_about_go"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/actionbar_go_normal" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/myhightView"
        android:layout_width="fill_parent"
        android:layout_height="40dip"
        android:layout_below="@+id/myexample"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/firsstTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="DatePick和timePicker"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/more_setting_about_go"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/actionbar_go_normal" />
    </RelativeLayout>

</RelativeLayout>